class Main {
    static int[] dp; // memoization array

    static int fib(int n) {
        if (n <= 1) return n;

        if (dp[n] != -1) return dp[n];  // already computed â†’ use it

        dp[n] = fib(n - 1) + fib(n - 2);
        return dp[n];
    }

    public static void main(String[] args) {
        int n = 40;
        dp = new int[n + 1];
        for (int i = 0; i <= n; i++) dp[i] = -1; // initialize

        System.out.println(fib(n)); // Fast!
    }
}
